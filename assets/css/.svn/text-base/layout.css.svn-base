.cell20{width: 20%;}
.cell3{width: 25%;}
.cell4{width: 33.3333%;}
.cell5{width: 41.6666%;}
.cell6{width: 50%;}
.cell7{width: 58.3333%;}
.cell8{width: 66.6666%;}
.cell9{width: 75%;}
.cell12{width: 100%;}

.cellLeft{width: 500px;}
.cellRight{width: 390px;}
.boxContent {padding:32px; }
/* >>> !Grid view */
.widePadding{padding: 50px;}

.alignRight{text-align:right!important;}
.alignCenter{text-align:center!important;}
.alignJustify{text-align:justify!important;}

.block{display:block!important;}
.inline{display:inline!important;}

.relative{ position: relative; }

.contactForm{margin-top: 62px;}

/* RING ICONS */
.ring{display: inline-block; font-family: 'open_sanssemibold'; font-size: 17px; text-align: center; color: #5873b1; margin: 8px 0;}
a.ring{color: #5873b1;}
.ring:before, .ring:after {content:" ";display: table;} .ring:after {clear: both;}
.ring:before{content: ""; display: block; background-color: #5571ae; background-position: center center; background-repeat: no-repeat; border:5px solid #a5b3d1; width: 96px; height: 96px; border-radius: 60%; margin: 0 auto 8px;}
.ring.leftIcon{text-align: left;}
.ring.leftIcon:before{float: left; margin: 0 10px 0 0;}
.ring.small{font-size: 14px;line-height: 22px; padding: 4px 0;}
.ring.small:before{width: 52px; height: 52px; border-width: 2px;}
.ring.small.leftIcon:before{margin: -6px 10px 0 0;}
.ring:hover:before{background-color: #223765; border-color:#8b96ad;}
.ring.red, a.ring.red{ /* color: #fd7a7a; */ }
.ring.red:before{background-color: #fd7a7a; border-color:#f8b7b7; }
.ring.red:hover:before{background-color: #ff6060; border-color:#f9aaaa; }
.ring.green, a.ring.green{color: #59d3bb; }
.ring.green:before{background-color: #59d3bb; border-color:#a7e4d8; }
.ring.green:hover:before{background-color: #35b39a; border-color:#95d4c7; }
.ring.white:before{background-color: #FFF; border-color:#5873b1; }
.ring.white:hover:before{background-color: #223765; border-color:#8b96ad; box-shadow: 0 0 0 4px #8b96ad; }

.ring.add:before{background-image: url(../images/icons/add-icon.png); background-size: 45px auto; }
.ring.edit:before{background-image: url(../images/icons/edit-icon.png); background-size: 50px auto; }
.ring.delete:before{background-image: url(../images/icons/delete-icon.png); background-size: 44px auto; }
.ring.search:before{background-image: url(../images/icons/search-icon.png); background-size: 44px auto; }
.ring.valid:before{background-image: url(../images/icons/valid-icon.png); background-size: 52px auto; }
.ring.certif:before{background-image: url(../images/icons/certif-icon.png); background-size: 53px auto; }
.ring.localiser:before{background-image: url(../images/icons/localiser-icon.png); background-size: 27px auto; }
.ring.globe:before{background-image: url(../images/icons/globe-icon.png); background-size: 40px auto; }
.ring.download:before{background-image: url(../images/icons/download-icon.png); background-size: 18px auto; }
.ring.download:hover:before{background-image: url(../images/icons/download-icon-hover.png); }
.ring.send:before{background-image: url(../images/icons/send-icon.png); background-size: 28px auto; }
.ring.send:hover:before{background-image: url(../images/icons/send-icon-hover.png); }

/* ICONS */
