@charset "utf-8";
/* CSS Document */
@import "fonts/OpenSans/stylesheet.css"; /* Importation du font-face */
@import "_form.css"; /* Importation du style form */
/* --- Général ---  */
*{margin:0; padding:0;}
html, body { height: 100% !important; overflow: visible!important; }
/* -ms-overflow-style: auto | none | scrollbar | -ms-autohiding-scrollbar */
body{font-family: 'open_sansregular'; font-size:14px; line-height: 26px; color: #373737; background-color: #f0f2f5;}

a{color:#5571ae;}
a:link {}
a:visited {}
a:hover {}
a:active {}

a img{ border:none;}

h1{font-family: 'open_sansextrabold'; font-size:31px; line-height: 38px; color:#5873b1; font-weight: normal; text-transform: uppercase; margin-bottom: 6px;}
h1 > span{font-family: 'open_sansregular';}
h2{font-family: 'open_sansregular'; font-size:31px; line-height: 36px; font-weight: normal; color:#5571ae; text-transform: uppercase; margin-bottom: 4px;}
h3{font-family: 'open_sanssemibold'; font-size:16px; color:#5571ae;line-height: 28px; margin-bottom: 8px;font-weight: normal;}
h4{font-family: 'open_sanssemibold'; font-size:28px;color: #373737;text-transform: uppercase; margin-bottom: 18px;}
h5{}
h6{}

p{ color: #8b8d94; margin: 0 0 8px;}
pre{white-space: pre-wrap; /* CSS 3 */white-space: -moz-pre-wrap; /* FF */ /* Opera 4~6 */white-space: -o-pre-wrap; /* Opera 7 */word-wrap: break-word; /* IE 5.5+ */}

em{ font-family: 'open_sansitalic'; font-style: normal; }

.mobile{display:none;}
.desktop{display:block;}

.defaultColor{ color:#5571ae; }
.lightColor{ color:#CCC; }
.black{color: #000;}

.regular{font-family: 'open_sansregular'!important;}
.light{font-family: 'open_sanslight'!important;}
.semi{font-family: 'open_sanssemibold'!important;}

.showInTablet{display: none;}
.showInMobile{display: none;}
.hide{display: none;}

.overflowHidden{ overflow: hidden!important; }
.absolute{ position: absolute!important; }

.pointer{ cursor:pointer; }

/* Floating Box */
.floating:after{display:block; content:''; clear:both;}
.left{float:left!important;}
.right{float:right!important;}
/* Clear Fix */
.clearfix:before,.clearfix:after {content:" ";display: table;}.clearfix:after {clear: both;}
.clr, .clear{clear:both;}
/* !Floating Box */

.ngdialog.ngdialog-theme-default .ngdialog-close::before{width: auto;}

/* >>> Grid view */
.grid{}
.grid:after, .mobileGrid:after {content: ".";display: block; clear: both;visibility: hidden; line-height: 0;  height: 0;}
.grid > [class^=cell], .mobileGrid > [class^=cell]{float: left; box-sizing:border-box; -webkit-box-sizing: border-box; -moz-box-sizing: border-box;}

.grid.expanded{ margin: 0 -4px; }
.grid.expanded > [class^=cell]{ padding: 0 4px;}
.rtl .grid > [class^=cell]{float: right;}

.tableGrid{display: table;width: 100%;box-sizing:border-box; -webkit-box-sizing: border-box; -moz-box-sizing: border-box;}
.tableGrid > [class^=cell]{float: none; display: table-cell; vertical-align: top;}

