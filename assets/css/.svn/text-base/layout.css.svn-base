/* loader */
#loader{ position: fixed; top:0; left:0; bottom: 0; right: 0; z-index: 11000; display: none; background: url(../images/loader.gif) no-repeat center center rgba(240,242,245,0.7);}
/* MODAL */
#modalScreen{position: fixed; top:0; left:0; bottom: 0; right: 0; z-index: 10000; background-color: rgba(240,242,245,0.7); display: flex; align-items: center; justify-content: center;}
#modalScreen.hidePopup, #modalScreen.hidePopup > #mainPopup{display: none;}
#mainPopup{ background: #fff;border: 1px solid #e2e6eb;  padding: 24px 24px 16px; /* display: table; margin: 0 auto; position: absolute;top: 25%;right: 0;  left: 0;*/}
#mainPopup .popupContent{ width: 400px; padding: 24px; font-size: 16px; line-height: 32px; color: #8b8d94; box-sizing:border-box; -webkit-box-sizing: border-box; -moz-box-sizing: border-box;}
#mainPopup .popupContent h3{margin-bottom: 40px;}
#mainPopup .popupContent p{margin-bottom: 20px;}
#mainPopup .closePopup{display: inline-block; float: right; text-decoration: none;}
